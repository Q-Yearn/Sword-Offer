# 
# @param number int整型 
# @return int整型
#
# class Solution:
#     def jumpFloorII(self , number ):
#         # write code here
#         dp = [1] * (number+1)
#         for i in range(2, number+1):
#             for j in range(1, i):
#                 dp[i] += dp[j]
#         return dp[number]


class Solution:
    def jumpFloorII(self , number ):
        state = [0] * (number+1)
        def dfs(cur):
            if cur == 0:
                return 1
            if cur < 0:
                return 0
            if state[cur] != 0:
                return state[cur]
            for i in range(1, cur+1):
                state[cur] += dfs(cur - i)
            return state[cur]
        return dfs(number)

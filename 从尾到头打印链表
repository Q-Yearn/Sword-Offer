# -*- coding:utf-8 -*-
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

# 方法一
# class Solution:
#     # 返回从尾部到头部的列表值序列，例如[1,2,3]
#     def printListFromTailToHead(self, listNode):
#         # write code here
#         head = listNode
#         res = []
#         while head:
#             res.append(head.val)
#             head = head.next
#         return res[::-1]


# 方法二
import sys
sys.setrecursionlimit(1000000)
class Solution:
    # 返回从尾部到头部的列表值序列，例如[1,2,3]
    def printListFromTailToHead(self, listNode):
        res = []
        def recursion(node):
            if node:
                recursion(node.next)
                res.append(node.val)
        head = listNode
        recursion(head)
        return res
        

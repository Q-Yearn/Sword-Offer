class Solution:
    def FindNumbersWithSum(self, array, tsum):
        tmp = set()
        res = []
        for num in array:
            if tsum - num not in tmp:
                tmp.add(num)
            else:
                res.append(tsum-num)
                res.append(num)
                break
        return res

#                     dfs(length+1, cur+str(numbers[i]))
#                     visited[i] = False
#         dfs(0, "")
#         return self.res

# 排序 直接冒泡排序都行 贪心思想  每次都是局部最大
class Solution:
    def PrintMinNumber(self , numbers ):
        if not numbers:
            return ""
        nums = list(map(str, numbers))
        cmp = lambda a, b : 1 if a + b > b + a else -1
        nums.sort(key = functools.cmp_to_key(cmp))
        return "".join(nums)

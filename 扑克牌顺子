#         if max(tmp) - min(tmp) >= 5:
#             return False
#         else:
#             return True


class Solution:
    def IsContinuous(self , numbers ):
        numbers.sort()
        zero = 0
        gap = 0
        for i in range(len(numbers)-1):
            if numbers[i] == 0:
                zero += 1
            else:
                if numbers[i+1] - numbers[i] == 0:
                    return False
                else:
                    gap += (numbers[i+1] - numbers[i] - 1)
        if gap > zero:
            return False
        else:
            return True

#             if num == index:
#                 return cur
#             cur += 1
            

class Solution:
    def GetUglyNumber_Solution(self , index ):
        if index <= 6:
            return index
        a, b, c = 0, 0, 0
        res = [1]
        for i in range(1, index):
            cur = min(res[a]*2, res[b]*3, res[c]*5)
            res.append(cur)
            if cur == res[a]*2:
                a += 1
            if cur == res[b]*3:
                b += 1
            if cur == res[c]*5:
                c += 1
        return res[index-1]

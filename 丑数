# @param index int整型 
# @return int整型
#
# 任意一个大于 1 的整数，都可以唯一分解成若干个质数的乘积
# import math
# class Solution:
#     def GetUglyNumber_Solution(self , index ):
#         num = 0
#         cur = 1
#         while True:
#             tmp = cur
#             while tmp % 2 == 0:
#                 tmp //= 2
#             while tmp % 3 == 0:
#                 tmp //= 3
#             while tmp %5 == 0:
#                 tmp //= 5
#             if tmp == 1:

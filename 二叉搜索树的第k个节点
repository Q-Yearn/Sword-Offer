#coding:utf-8
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None
#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param proot TreeNode类 
# @param k int整型 
# @return int整型
#
class Solution:
    def KthNode(self , proot , k ):
        # write code here
        cur = proot
        stack = []
        while cur or stack:
            while cur:
                stack.append(cur)
                cur = cur.left
            
            k -= 1
            cur = stack.pop()
            if k == 0:
                return cur.val
            cur = cur.right
        return -1
            

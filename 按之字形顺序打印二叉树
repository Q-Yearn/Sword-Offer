class Solution:
    def Print(self , pRoot ):
        if not pRoot:
            return []
        stack1 = [pRoot]
        stack2 = []
        flag = 1
        res = []
        while stack1 or stack2:
            tmp = []
            if flag:
                size = len(stack1)
                for i in range(size):
                    node = stack1.pop()
                    tmp.append(node.val)
                    if node.left:
                        stack2.append(node.left)
                    if node.right:
                        stack2.append(node.right)
                flag = 0
            else:
                size = len(stack2)
                for i in range(size):
                    node = stack2.pop()
                    tmp.append(node.val)
                    if node.right:
                        stack1.append(node.right)
                    if node.left:
                        stack1.append(node.left)
                flag = 1
            res.append(tmp)
        return res

        
    
        

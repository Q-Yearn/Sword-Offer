#coding:utf-8
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None
#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param root TreeNode类 
# @param o1 int整型 
# @param o2 int整型 
# @return int整型
#
# 见JZ68 题目是二叉搜索树
# 此题为普通二叉树
# 方法一  记录包含两个节点的路径  然后看两个和路径中的最先共同节点
class Solution:
    def lowestCommonAncestor(self , root , o1 , o2 ):
        if not root:
            return -1
        if root.val == o1 or root.val == o2:
            return root.val
        left = self.lowestCommonAncestor(root.left, o1, o2)
        right = self.lowestCommonAncestor(root.right, o1, o2)

        if left == -1:
            return right
        if right == -1:
            return left
        return root.val

#                 while cur.next and cur.next.val == tmp:
#                     cur.next = cur.next.next
#             else:
#                 cur = cur.next
#         return res.next


class Solution:
    def deleteDuplication(self, pHead):
        if not pHead:
            return None
        tmp = {}
        node = pHead
        while node:
            if node.val in tmp:
                tmp[node.val] += 1
            else:
                tmp[node.val] = 1
            node = node.next
        res = ListNode(0)
        res.next = pHead
        cur = res
        while cur.next:
            if tmp[cur.next.val] != 1:
                cur.next = cur.next.next
            else:
                cur = cur.next
        return res.next


class Solution:
    def Print(self , pRoot ):
        res = []
        def dfs(root, res, depth):
            if root:
                if len(res) < depth:
                    res.append([])
                res[depth - 1].append(root.val)
                dfs(root.left, res, depth+1)
                dfs(root.right, res, depth+1)
        dfs(pRoot, res, 1)
        return res

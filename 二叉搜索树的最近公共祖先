#
# 
# @param root TreeNode类 
# @param p int整型 
# @param q int整型 
# @return int整型
#
# 使用返回值或者使用flag来及时停止递归
# class Solution:
#     def lowestCommonAncestor(self , root , p , q ):
#         # write code here
#         res1 = []
#         res2 = []
#         res = None
#         def dfs(node, res, target):
#             if not node:
#                 return False
#             res.append(node)
#             if node.val == target:
#                 return True
#             if dfs(node.left, res, target):
#                 return True
#             if dfs(node.right, res, target):
#                 return True
#             res.pop()
#             return False
#         dfs(root, res1, p)
#         dfs(root, res2, q)
#         for a, b in zip(res1, res2):
#             if a == b:
#                 res = a
#             else:
#                 break
#         return res.val

class Solution:
    def lowestCommonAncestor(self , root , p , q ):
        if not root:
            return -1
        if p >= root.val and q <= root.val or p <= root.val and q >= root.val:
            return root.val
        elif p <= root.val and q <= root.val:
            return self.lowestCommonAncestor(root.left, p, q)
        else:
            return self.lowestCommonAncestor(root.right, p, q)

class Solution:
    def reOrderArray(self , array ):
        for i in range(len(array)):
            if array[i] % 2 == 1:
                j = i
                while j > 0 and array[j-1] % 2 == 0:
                    array[j], array[j-1] = array[j-1], array[j]
                    j -= 1
        return array

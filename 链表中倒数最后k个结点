# 快慢指针
class Solution:
    def FindKthToTail(self , pHead , k ):
        slow = pHead
        fast = pHead
        for i in range(k):
            if not fast:
                return None
            fast = fast.next
        while fast:
            fast= fast.next
            slow = slow.next
        return slow
